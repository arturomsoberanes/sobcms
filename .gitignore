# Ignore directories and files generated by Composer
/vendor/
/composer.phar
/composer.lock

# Ignore files and directories generated by the development environment
/.idea/
/.vscode/
/.phpunit.result.cache
/.phpunit.result.cache.lock

# Ignore environment configuration files
.env
.env.*

# Ignore cache and log files
/log/
/cache/

# Ignore backup files
*.bak
*.swp

# Ignore development dependencies files
/devDependencies/

# Ignore specific files and directories of your project
/path/to/file.ext
/path/to/directory/

# Ignore custom configuration files (example)
/config.php

# Ignore test fixture files (example)
/tests/fixtures/

# Ignore sensitive files
/secret-key.txt
/database-credentials.json

# Ignore files in media_upload

/media_upload/*
!/media_upload/.gitkeep